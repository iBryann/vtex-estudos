{
    "store.home": {
        "blocks": [
            "slider-programavel#demo",
            // "stack-layout#cta",
            "responsive-layout.desktop#desktop",
            "responsive-layout.mobile#mobile",
            "flex-layout.row#slider-brands", // Estou importando esse template do arquivo slider-layout.jsonc ðŸ˜®
