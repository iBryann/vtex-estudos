    "stack-layout#cta": {
        "props": {
            "blockClass": "cta"
        },
        "children": [
            "image#cta",
            "link#cta"
        ]
    },
    "image#cta": {
        "props": {
            "blockClass": "cta",
            "width": "100%",
            "height": 400,
            "src": "https://via.placeholder.com/1048x257/8fc3ff/fff?text=Â "
        }
    },
    "link#cta": {
        "props": {
            "blockClass": "cta",
            "displayMode": "button",
            "buttonProps": {
                "variant": "primary",
                "size": "large"
            },
            "href": "/iogurte",
            "label": "Check these awesome discounts"
        }
    },
    "responsive-layout.desktop#desktop": {
        "children": [
            "rich-text#desktop",
            "image#desktop"
        ]
    },
    "responsive-layout.mobile#mobile": {
        "children": [
            "rich-text#mobile",
            "image#mobile"
        ]
    },
    "rich-text#desktop": {
        "props": {
            "text": "# Your Coffee, Your Way \n ### New Coffee Makers Collection",
